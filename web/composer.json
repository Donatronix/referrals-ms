{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2",
        "ext-bcmath": "*",
        "ext-json": "*",
        "darkaonline/swagger-lume": "~6",
        "doctrine/dbal": "~2",
        "flipbox/lumen-generator": "~6",
        "guzzlehttp/guzzle": "~6",
        "kreait/laravel-firebase": "~2",
        "laravel/lumen-framework": "~6",
        "sumra/baum": "*",
        "sumra/json-api": "*",
        "sumra/pubsub": "*",
        "vladimir-yuldashev/laravel-queue-rabbitmq": "~9",
        "zircote/swagger-php": "~3",
        "ext-openssl": "*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1",
        "phpunit/phpunit": "~8",
        "mockery/mockery": "~1"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type" : "path",
            "url" : "../pubsub",
            "options": {
                "symlink": false
            }
        },
        {
            "type": "path",
            "url": "../json-api",
            "options": {
                "symlink": false
            }
        },
        {
            "type": "path",
            "url": "../baum",
            "options": {
                "symlink": false
            }
        }
    ]
}
